# Keystore configuration for release signing
# Copy this file to keystore.properties and fill in your actual values

storePassword=your_store_password
keyPassword=your_key_password
keyAlias=your_key_alias
storeFile=../keystore/release-keystore.jks

# Optional: Upload keystore for Play Store
uploadStorePassword=your_upload_store_password
uploadKeyPassword=your_upload_key_password
uploadKeyAlias=your_upload_key_alias
uploadStoreFile=../keystore/upload-keystore.jks
